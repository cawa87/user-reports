{
  "name": "userreports-server",
  "version": "1.0.0",
  "description": "Backend API for UserReports application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "dotenv -e ../.env -- ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "dotenv -e ../.env -- prisma generate",
    "db:push": "dotenv -e ../.env -- prisma db push",
    "db:migrate": "dotenv -e ../.env -- prisma migrate dev",
    "db:seed": "dotenv -e ../.env -- ts-node src/scripts/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.2",
    "redis": "^4.6.8",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.9",
    "@types/node-cron": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prisma": "^5.3.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
